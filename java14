# install 
curl -O https://download.java.net/java/GA/jdk14.0.2/205943a0976c4ed48cb16f1043c5c647/12/GPL/openjdk-14.0.2_linux-x64_bin.tar.gz


tar xvf openjdk-14.0.2_linux-x64_bin.tar.gz
mv jdk-14.0.2/ /opt


# create a jdk4.sh on /etc/profile.d/jdk14.sh
export JAVA_HOME=/opt/jdk-14.0.2
export PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/root/bin:/bin:/opt/jdk-14.0.2/bin
